{
	"compilerOptions": {
		"jsx": "react",
		"sourceMap": true,
		"target": "es5",
		"types": [
			"node"
		],
		"typeRoots": [
			"./node_modules/@types"
		],
		"baseUrl": ".",
		"paths": {
			"_page/*": 		["src/Pages/*"],
			"_component/*": ["src/components/*"],
			"_container/*": ["src/containers/*"],
			"_ui/*": ["src/components/ui/*"]
		}
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		"typings/index.d.ts"
	]
}